package even_position;

public class even_Position {

	public static void main(String[] args) {
		int a[]=new int[] {1,2,3,4,5,6,7,8,};
		for(int i=1; i<a.length;i=i+2) {
			System.out.println("Intex"+ i);
			System.out.println("value is"+ a[i]);
		}

	}

}



output;

Intex1
value is2
Intex3
value is4
Intex5
value is6
Intex7
value is8
