import java.util.Scanner;

public class dsdsd {
public static void main(String[] args) {
	int []a=new int [] {1,2,3,4,5,6,7,8};
	for(int i=2;i<a.length;i=i+2) {
		System.out.println("even position"+i);
		System.out.println("volue is"+a[i]);
	}
		
	}
}


output

even position2
volue is3
even position4
volue is5
even position6
volue is7
