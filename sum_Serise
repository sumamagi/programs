package sum_serise;

import java.util.Scanner;

public class Sum_Serise {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number");
		int n=sc.nextInt();
		int sum=0,p=1;
		for(int i=0;i<n;i++) {
			sum=sum+p;
			if(n==i+1)
			System.out.print(p);
			else
				System.out.print(p+"+");
			p=(p*10)+1;
			
		}
			
	
	

	}
	
}

output;

Enter the number
5
1+11+111+1111+11111
