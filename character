package character;

import java.util.Scanner;

public class character {

	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	System.out.print("Enter the character");
	char ch=sc.next().charAt(0);
	for (  ch=ch;ch<='z';ch++) {
		System.out.print(ch +"");
	}

	}

}

output;

