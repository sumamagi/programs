package largest_element;

public class largest {

	public static void main(String[] args) {
		int []a= {4,5,6,7,56,45};
		int largest=a[0];
		for(int i=0; i<a.length;i++)
		{
			if (largest<a[i])
			{
				largest=a[i];
			
			}
		}
System.out.println("largest number is" + largest);
	}

}





output;
largest number is56



