package program_ex;

public class Qustion_72_anather {

	public static void main(String[] args) {
		int []array= {1,2,3,3,4,5,5,6,7,7};
		int j=0;
		for(int i=0;i<array.length-1;i++) {
			if(array[i]!=array[i+1]) {
				array[j++]=array[i];
			}
		}
		array[j]=array[array.length-1];
		for(int l=0;l<j+1;l++) {
			System.out.print(array[l]+" ");
		}

	}

}

output;

1 2 3 4 5 6 7 
