package program_ex;

public class Qustion_73_ {

	public static void main(String[] args) {
		int a[][]= {{1,2,3},{4,5,6},{7,8,9}};
		int b[][]= {{1,2,1},{1,3,4},{2,3,4}};
int c[][]=new int[3][3];
for(int i=0;i<3;i++) {
	for(int j=0;j<3;j++) {
		c[i][j]=a[i][j]+b[i][j];
		System.out.print(c[i][j]+" ");
	}
	System.out.println();
}
	}

}

output;
2 4 4 
5 8 10 
9 11 13 
