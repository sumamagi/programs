package patten_8;

import java.util.Scanner;

public class Pattem_8 {

	public static void main(String[] args) {
		Scanner sc=new Scanner (System.in);
		System.out.println("Enter the number");
		int n=sc.nextInt();
		int i;
		for(i=1;i<=n;i++) {
			for(int j=1;j<=n;j++) {
				if((j==1)||(i==1||i==n)&&(j>1)){

	System.out.print("*");
}
				else {
					System.out.print("");
				}
			}
			System.out.println();
		}

	}

}

output;
Enter the number
5
*****
*
*
*
*****
