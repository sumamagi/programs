package odd_Position_Array;

public class odd_Position_Array {

	public static void main(String[] args) {
		int a[]=new int [] {1,2,3,4,5,6,7,8,};
		
		for(int i=0; i<a.length;i=i+2) {
			System.out.println("Even position"+i);
			System.out.println("valu is"+a[i]);
		}

	}

}


output;

odd position0
valu is1
odd position2
valu is3
odd position4
valu is5
odd position6
valu is7
