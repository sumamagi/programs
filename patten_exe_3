package patten5;

public class patten5 {

	public static void main(String[] args) {
	int n=9;
		for(int i=1,p=9; i<=n;i++,p--) {
			for(int j=1;j<i;j++) {
				System.out.print(" ");
			
			
			
				}
				for(int j=i;j<=n;j++) {
					System.out.print(p+" ");
				}
			
			System.out.println();
		}

	}

	}
  
  output;
  
9 9 9 9 9 9 9 9 9 
 8 8 8 8 8 8 8 8 
  7 7 7 7 7 7 7 
   6 6 6 6 6 6 
    5 5 5 5 5 
     4 4 4 4 
      3 3 3 
       2 2 
        1 
